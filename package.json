{
  "author": "brandon@fabrikant.tech",
  "bin": {
    "designbase": "./bin/run.js",
    "designbase-dev": "./bin/dev.js"
  },
  "bugs": "https://github.com/fabrikant-tech/cli/issues",
  "dependencies": {
    "@inquirer/prompts": "7.5.3",
    "@oclif/core": "4.4.0",
    "@oclif/plugin-help": "6.2.29",
    "diff": "8.0.2",
    "globby": "14.1.0",
    "lodash-es": "4.17.21",
    "multimatch": "7.0.0",
    "node-fetch": "3.3.2",
    "node-pager": "0.3.6",
    "semver": "7.7.2",
    "socket.io-client": "4.8.1",
    "tty-table": "4.2.3",
    "zod": "3.25.67"
  },
  "description": "Designbase CLI",
  "devDependencies": {
    "@eslint/compat": "1.3.1",
    "@oclif/test": "4.1.13",
    "@stylistic/eslint-plugin": "5.0.0",
    "@types/chai": "4.3.3",
    "@types/lodash-es": "4.17.12",
    "@types/mocha": "10.0.10",
    "@types/node": "22.9.0",
    "@types/semver": "7.7.0",
    "chai": "4.5.0",
    "eslint": "9.29.0",
    "eslint-plugin-collation": "1.5.0",
    "mocha": "10.8.2",
    "oclif": "4.20.1",
    "prettier": "3.6.1",
    "shx": "0.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/fabrikant-tech/cli",
  "keywords": [
    "designbase",
    "cli"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "name": "@designbase/cli",
  "oclif": {
    "bin": "designbase",
    "commands": "./dist/commands",
    "dirname": "designbase",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": "fabrikant-tech/cli",
  "scripts": {
    "clean": "shx rm -rf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc -b",
    "build:watch": "npm run clean && tsc --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "0.0.0"
}
